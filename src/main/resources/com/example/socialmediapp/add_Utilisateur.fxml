<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="443.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chat.AddUtilisateurController">
   <children>
      <VBox fx:id="vbox" layoutX="-6.0" layoutY="-3.0" prefHeight="443.0" prefWidth="360.0">
         <children>
            <HBox prefHeight="47.0" prefWidth="360.0">
               <children>
                  <ImageView fx:id="search_button" fitHeight="32.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true" translateX="5.0" translateY="7.0">
                     <image>
                        <Image url="@images/search_icon.jpeg" />
                     </image>
                  </ImageView>
                  <TextField fx:id="search" prefHeight="31.0" prefWidth="306.0" promptText="Type a Username" style="-fx-background-radius: 160px;" translateX="7.0" translateY="9.0" />
               </children>
            </HBox>
            <ScrollPane fx:id="scrollPane" prefHeight="394.0" prefWidth="360.0">
               <content>
                  <VBox fx:id="scrollContent" prefHeight="400.0" prefWidth="360.0" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
